{
  "name": "psygo",
  "version": "1.0.0",
  "description": "The easiest way to get started creating jsPsych plugins!",
  "main": "lib/index.js",
  "bin": {
    "psygo": "lib/index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p .",
    "local": "npm run build && sudo npm i -g",
    "create": "npm run build && npm run test",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henry-burgess/psygo.git"
  },
  "keywords": [
    "jspsych",
    "neuroscience",
    "gorilla"
  ],
  "author": "Henry Burgess",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/henry-burgess/psygo/issues"
  },
  "homepage": "https://github.com/henry-burgess/psygo#readme",
  "devDependencies": {
    "@types/node": "^14.14.13",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/inquirer": "^7.3.1",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.2.1",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "path": "^0.12.7"
  }
}
